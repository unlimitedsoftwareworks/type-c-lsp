// Test case for array type inference error validation

type Point = struct {
    x: f32,
    y: f32[]
}

fn test1() {
    // This should show a validation error because y has different types:
    // - First element: y is f32[] (correct)
    // - Second element: y is string (incorrect)
    let points: Point[] = [
        {x: 0.0f, y: [0.0f]},
        {x: 1.0f, y: ["gi"]}
    ]
}

fn test2() {
    // This should also show an error in the array literal itself
    // because it cannot infer a common struct type
    let points = [
        {x: 0.0f, y: [0.0f]},
        {x: 1.0f, y: "gi"}
    ]
}

fn test3() {
    // Empty array without type annotation should show error
    let arr = []
}