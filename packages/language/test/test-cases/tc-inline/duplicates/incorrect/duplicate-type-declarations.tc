    type Size = u64
}

// Nested namespace with duplicates
namespace Outer {
    type Data = u32
    /// @Error(TCE088): Duplicate type 'Data'
    type Data = f32
    
    namespace Inner {
        type Info = string
        /// @Error(TCE088): Duplicate type 'Info'
        type Info = u32
    }
}

