type vec3 = struct {
    x: f32, y: f32, z: f32
}

type Object3D  = interface {
    fn getPos() -> vec3
    fn getScale() -> vec3
    fn getRotation() -> vec3
}

type Default3DImpl<T> = impl Object3D (
    position: T,
    scale: T,
    rot: T
) {
    fn getPos() = this.position
    fn getScale() = this.scale
    fn getRotation() = this.rot
}

type Mesh = class {
    let pos: vec3
    let scale: vec3
    let rot: vec3

    /// @Error(TCE061): Duplicate class method
    impl Default3DImpl<vec3>(pos, scale, rot)

    fn getPos() -> vec3 = this.pos
}

fn main() {
    let m: Object3D = new Mesh()
    let z = m.getPos()
}