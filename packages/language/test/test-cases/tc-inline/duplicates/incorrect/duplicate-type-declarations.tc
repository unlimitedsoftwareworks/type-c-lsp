    type Vector = struct { x: f32, y: f32 }
    /// @Error(TCE088): Duplicate type 'Vector'
    type Vector = struct { x: f64, y: f64 }
}

// Multiple duplicates in namespace
namespace Graphics {
    type Color = u32
    type Size = struct { width: u32, height: u32 }
