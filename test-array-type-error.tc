// Test: Array with incompatible types should show validation error

fn test() {
    // This should show error: Cannot infer common type
    let arr1 = [[1u32, 2u32], null, [null]];
    
    // This should also show error
    let arr2 = [1, "string", true];
    
    // This should work fine
    let arr3 = [1u32, 2u32, 3u32];
}