// Test duplicate type declarations - should report errors

// Duplicate types in module
type Point = u32
/// @Error(TCE088): Duplicate type 'Point'
type Point = f32

// Multiple duplicates
type Vec2 = struct { x: u32, y: u32 }
type Vec3 = struct { x: u32, y: u32, z: u32 }
/// @Error(TCE088): Duplicate type 'Vec2'
type Vec2 = struct { x: f32, y: f32 }
