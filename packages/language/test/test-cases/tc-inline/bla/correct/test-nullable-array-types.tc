fn main() {
    // Array of nullable structs: {x: u32, y: u32}?[]
    let points1: {x: u32, y: u32}?[] = [{x: 1u32, y: 2u32}, null, {x: 3u32, y: 4u32}]
    
    // Nullable array of structs: {x: u32, y: u32}[]?
    let points2: {x: u32, y: u32}[]? = [{x: 1u32, y: 2u32}, {x: 3u32, y: 4u32}]
    let points3: {x: u32, y: u32}[]? = null
    
    // Array of nullable primitives
    let nums1: u32?[] = [1u32, null, 3u32]
    
    // Nullable array of primitives
    let nums2: u32[]? = [1u32, 2u32, 3u32]
    let nums3: u32[]? = null
    
    // More complex examples
    let nested1: u32?[]? = null
    let nested2: u32?[]? = [1u32, null, 3u32]
    let nested3: ((u32[])?)[] = [[1u32, 2u32], null, [null]]
}